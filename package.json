{
    "name": "gas-app-framework",
    "version": "1.0.0",
    "private": true,
    "exports": {
        "./core": "./src/core/index.ts",
        "./testing/common": "./src/testing/common/index.ts",
        "./testing/gas": "./src/testing/gas/index.ts",
        "./testing/node": "./src/testing/node/index.ts"
    },
    "scripts": {
        "lint": "eslint ./src ./test",
        "lint:fix": "eslint ./src ./test --fix",
        "build:test-modules": "ts-node --project build/tsconfig.json build/transform-namespace-to-module.ts",
        "pretest:node": "npm run build:test-modules",
        "test:node": "jest",
        "test:node:shared": "jest test/node/shared/",
        "test:node:integration": "jest test/node/integration/",
        "test:node:unit": "jest test/node/unit/",
        "test:gas": "echo 'Run in Apps Script: test_RunAll()'"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/jest": "^30.0.0",
        "@types/node": "^25.0.3",
        "@typescript-eslint/eslint-plugin": "^8.50.0",
        "@typescript-eslint/parser": "^8.50.0",
        "eslint": "^9.39.2",
        "jest": "^30.2.0",
        "ts-jest": "^29.4.6",
        "ts-morph": "^27.0.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.50.0"
    },
    "dependencies": {
        "@types/google-apps-script": "^2.0.8"
    }
}
