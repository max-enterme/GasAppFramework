{
    "name": "@max-enterme/gas-app-framework-modules",
    "version": "0.0.0",
    "private": true,
    "description": "GasAppFramework modules (runtime JS + TypeScript types) for bundling into Google Apps Script projects",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist/**",
        "README.md"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
        "prepare": "npm run build",
        "prepack": "npm run clean && npm run build"
    },
    "devDependencies": {
        "typescript": "^5.0.0"
    }
}
